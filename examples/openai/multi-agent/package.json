{
  "name": "openai-multi-agent-typescript",
  "version": "1.0.0",
  "description": "Multi-agent translation system built with TypeScript and OpenAI",
  "type": "module",
  "main": "main.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsx main.ts",
    "start:mcp": "tsx run_mcp.ts",
    "start:mcp:sse": "tsx run_mcp.ts sse",
    "dev": "tsx --watch main.ts",
    "test": "tsx main.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "openai": "^4.103.0",
    "dotenv": "^16.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "openai",
    "agents",
    "typescript",
    "translation",
    "multi-agent",
    "mcp"
  ],
  "author": "",
  "license": "MIT"
} 